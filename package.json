{
  "name": "silk-auth",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "type": "module",
  "scripts": {
    "dev": "pnpm install && prisma generate && next dev",
    "build": "NODE_ENV=production next build && cat prisma/schema.prisma && cat .generated/prisma/client/index.js",
    "build:vercel": "prisma generate && NODE_ENV=production next build",
    "dev:db": "prisma format && prisma migrate dev",
    "dev:db:seed": "prisma db seed",
    "start": "next start",
    "lint": "next lint && tsc --noEmit && rm -rf .next/cache/eslint/ ; prettier -c \"./**/*.{ts,tsx,js,jsx,mdx}\" .",
    "test": "vitest run",
    "prepare": "husky",
    "publish": "vercel publish",
    "format:write": "prettier --write \"./**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "chown": "stat -c '%u %g' /app/package.json | awk '{printf \"chown -R %i:%i /app/\", $1,$2}' | bash",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.14",
    "@silk-wallet/silk-interface-core": "^0.4.2",
    "@silk-wallet/silk-wallet-sdk": "^0.7.1",
    "@tanstack/react-query": "^5.80.10",
    "@tanstack/react-query-devtools": "^5.80.10",
    "@wagmi/core": "^2.17.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "ethers": "6.14.3",
    "husky": "^9.1.7",
    "lucide-react": "^0.511.0",
    "next": "^15.3.4",
    "next-auth": "5.0.0-beta.28",
    "react": "^19.1.0",
    "react-auth": "^1.0.0",
    "react-dom": "^19.1.0",
    "siwe": "^3.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.5",
    "viem": "^2.31.3",
    "wagmi": "^2.15.6",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint-community/eslint-utils": "^4.7.0",
    "@types/node": "20.10.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.3.4",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "prisma": "^6.10.1",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vercel": "^41.7.8",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
