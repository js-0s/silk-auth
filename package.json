{
  "name": "silk-auth",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "type": "module",
  "scripts": {
    "dev": "pnpm install && prisma generate && next dev",
    "build": "NODE_ENV=production next build && cat prisma/schema.prisma && cat .generated/prisma/client/index.js",
    "build:vercel": "prisma generate && NODE_ENV=production next build",
    "dev:db": "prisma format && prisma migrate dev",
    "dev:db:seed": "prisma db seed",
    "start": "next start",
    "lint": "next lint && tsc --noEmit && rm -rf .next/cache/eslint/ ; prettier -c \"./**/*.{ts,tsx,js,jsx,mdx}\" .",
    "test": "vitest run",
    "prepare": "husky",
    "publish": "vercel publish",
    "format:write": "prettier --write \"./**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "chown": "stat -c '%u %g' /app/package.json | awk '{printf \"chown -R %i:%i /app/\", $1,$2}' | bash",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.14",
    "@silk-wallet/silk-interface-core": "^0.4.0",
    "@silk-wallet/silk-wallet-sdk": "^0.6.0",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@wagmi/core": "^2.17.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "ethers": "6.14.3",
    "husky": "^9.1.7",
    "lucide-react": "^0.511.0",
    "next": "^15.3.3",
    "next-auth": "5.0.0-beta.28",
    "prisma": "^6.1.0",
    "react": "^19.1.0",
    "react-auth": "^1.0.0",
    "react-dom": "^19.1.0",
    "siwe": "^3.0.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.4",
    "viem": "^2.22.3",
    "wagmi": "^2.13.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint-community/eslint-utils": "^4.6.1",
    "@types/node": "20.10.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.0.3",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vercel": "^41.7.3",
    "vitest": "^3.1.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
